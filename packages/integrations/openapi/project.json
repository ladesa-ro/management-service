{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "tags": ["integrations", "integrations:openapi"],
  "namedInputs": {
    "default": []
  },
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["{workspaceRoot}/packages/service/.dist/openapi.v3.json"],
      "options": {
        "cwd": "{projectRoot}",
        "commands": ["mkdir -p ./lib", "cp ../../service/.dist/openapi.v3.json ./lib/openapi.v3.json"],
        "parallel": false
      },
      "outputs": ["{projectRoot}/lib/**/*"],
      "dependsOn": ["@ladesa-ro/management-service:build"]
    }
  }
}
