    "unplugin-swc": "^1.5.5",
    "vite-tsconfig-paths": "catalog:dev",
    "vitest": "catalog:test"
  },
  "scripts": {
    "application:build": "nx application:build --outputStyle stream",
    "application:dev": "nx application:dev --outputStyle stream",
    "application:debug": "nx application:debug --outputStyle stream",
    "contracts:domain:build": "nx contracts:domain:build --outputStyle stream",
    "contracts:domain:dev": "nx contracts:domain:dev --outputStyle stream",
    "contracts:application:build": "nx contracts:application:build --outputStyle stream",
    "contracts:application:dev": "nx contracts:application:dev --outputStyle stream",
    "dev": "bun run application:dev",
    "build": "bun run application:build",
    "start": "bun run lib/main.ts",
    "check:static": "tsc --noEmit",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:watch": "vitest",
    "typeorm": "typeorm",
    "typeorm:create": "bun run typeorm migration:create",
    "typeorm:ds-migration": "bun run typeorm -d lib/infrastructure/integrations/database/typeorm/data-sources/migration.data-source.ts",
    "typeorm:generate": "bun run typeorm migration:generate",
    "migration:revert": "bun run typeorm:ds-migration migration:revert",
    "migration:run": "bun run typeorm:ds-migration migration:run"
  },
  "_moduleAliases": {
    "@": "./lib",
    "@/infrastructure": "./lib/infrastructure"
  }
}
