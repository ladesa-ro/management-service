    "@types/express": "catalog:infrastructure/express",
    "@types/graphql-list-fields": "catalog:infrastructure/graphl",
    "@types/json-schema": "catalog:infrastructure/validation/json-schema",
    "@types/lodash": "catalog:infrastructure/utils",
    "@types/multer": "catalog:infrastructure/express",
    "@types/passport-http-bearer": "catalog:infrastructure/express/passport",
    "@types/rascal": "catalog:infrastructure/integrations/amqp",
    "@types/supertest": "catalog:test",
    "@types/uuid": "catalog:infrastructure/utils",
    "@typespec/compiler": "catalog:dev/typespec",
    "@typespec/http": "catalog:dev/typespec",
    "@typespec/json-schema": "catalog:dev/typespec",
    "@typespec/openapi3": "catalog:dev/typespec",
    "@typespec/rest": "catalog:dev/typespec",
    "@vitest/coverage-v8": "catalog:test",
