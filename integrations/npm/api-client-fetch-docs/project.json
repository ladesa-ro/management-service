{
  "tags": ["integrations", "integrations:npm"],
  "root": "integrations/npm/api-client-fetch-docs",
  "namedInputs": {
    "default": ["{projectRoot}/.config/**/*"]
  },
  "targets": {
    "watch": {
      "cache": false,
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "typedoc --options .config/typedoc.jsonc --watch --preserveWatchOutput"
      },
      "dependsOn": []
    },
    "serve": {
      "cache": false,
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "serve dist"
      },
      "dependsOn": ["build"]
    },
    "build": {
      "cache": true,
      "executor": "nx:run-commands",
      "inputs": ["default", "^default"],
      "options": {
        "cwd": "{projectRoot}",
        "command": "typedoc --options .config/typedoc.jsonc"
      },
      "outputs": [],
      "dependsOn": ["@ladesa-ro/api-client-fetch:generate"]
    }
  }
}
