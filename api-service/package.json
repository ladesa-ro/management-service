{
  "name": "@ladesa-ro/api.service",
  "version": "1.0.0-next.29",
  "private": true,
  "files": ["**/*"],
  "scripts": {
    "build:nest": "nest build",
    "build:openapi-json": "OUT_FILE=./dist/generated.json pnpm run generate:openapi",
    "build": "pnpm run build:nest && pnpm run build:openapi-json",
    "generate:openapi": "node --experimental-require-module dist/scripts/codegen-openapi-json",
    "check:static": "tsc --noEmit",
    "start": "nest start -e 'node --experimental-require-module --trace-warnings'",
    "start:dev": "GIT_COMMIT_HASH=local nest start --watch -e 'node --experimental-require-module --trace-warnings'",
    "start:debug": "GIT_COMMIT_HASH=local nest start --debug 0.0.0.0:9229 --watch -e 'node --experimental-require-module --trace-warnings'",
    "start:prod": "node --experimental-require-module dist/main",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "typeorm:ds-migration": "tsx ./node_modules/typeorm/cli.js -d src/infrastructure/integrations/database/typeorm/data-sources/migration.data-source.ts",
    "typeorm:entity": "pnpm run typeorm entity:create",
    "typeorm:generate": "pnpm run typeorm migration:generate",
    "typeorm:create": "pnpm run typeorm migration:create",
    "migration:run": "pnpm run typeorm:ds-migration migration:run",
    "migration:revert": "pnpm run typeorm:ds-migration migration:revert",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@apollo/utils.keyvaluecache": "^3.1.0",
    "@fastify/merge-json-schemas": "^0.2.1",
    "@keycloak/keycloak-admin-client": "^25.0.6",
    "@ladesa-ro/especificacao": "^3.3.11",
    "@nestjs/apollo": "^13.0.2",
    "@nestjs/common": "^11.0.4",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.4",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/graphql": "^13.0.2",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/platform-express": "^11.0.4",
    "@nestjs/schedule": "^5.0.0",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/throttler": "^6.4.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "amqplib": "^0.10.5",
    "change-case": "^5.4.4",
    "compression": "^1.7.5",
    "date-fns": "^4.1.0",
    "defu": "^6.1.4",
    "fs-jetpack": "^5.1.0",
    "graphql": "^16.10.0",
    "graphql-list-fields": "^2.0.4",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^11.0.2",
    "nestjs-paginate": "^10.0.0",
    "openid-client": "^6.1.7",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.13.1",
    "rascal": "^20.1.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5",
    "valibot": "1.0.0-beta.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.4",
    "@swc/cli": "0.6.0",
    "@swc/core": "^1.10.9",
    "@types/amqplib": "^0.10.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/graphql-list-fields": "^2.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.17.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport-http-bearer": "^1.0.41",
    "@types/rascal": "^10.2.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.0.3",
    "ast-types": "^0.16.1",
    "esbuild": "^0.24.2",
    "magicast": "^0.3.5",
    "pathe": "^2.0.2",
    "recast": "^0.23.9",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.3"
  }
}
