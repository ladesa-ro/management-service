    "typeorm:generate": "bun run typeorm migration:generate",
    "migration:revert": "bun run typeorm:ds-migration migration:revert",
    "migration:run": "bun run typeorm:ds-migration migration:run",
    "code:check": "biome check .",
    "code:check:format": "biome format",
    "code:check:lint": "biome lint",
    "code:fix": "biome check --write --unsafe",
    "code:fix:format": "biome format --write --unsafe",
    "code:fix:lint": "biome lint --write --unsafe"
