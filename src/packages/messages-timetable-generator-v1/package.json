{
  "name": "@ladesa-ro/messages.timetable-generator.v1",
  "version": "0.0.1",
  "private": true,
  "main": "./src/schemas.ts",
  "types": "./src/schemas.ts",
  "exports": {
    ".": "./src/schemas.ts"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "quicktype": "^23.2.6"
  },
  "scripts": {
    "codegen:fresh": "nx run @ladesa-ro/messages.timetable-generator.v1:codegen",
    "codegen:cold": "echo 'pulando geração do schemas.ts'"
  },
  "nx": {
    "targets": {
      "codegen": {
        "executor": "nx:run-commands",
        "cache": true,
        "inputs": [
          "{projectRoot}/.commands/generate"
        ],
        "outputs": [
          "{projectRoot}/src/schemas.ts"
        ],
        "options": {
          "command": "bash .commands/generate",
          "cwd": "{projectRoot}"
        }
      }
    }
  }
}
